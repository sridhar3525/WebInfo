<p id="notice"><%= notice %></p>

<h1>User Details</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Firstname</th>
      <th>Middlename</th>
      <th>Lastname</th>
      <th>Emailaddress</th>
      <th>Password</th>
      <th>Phoneno</th>
      <th>Dateofbirth</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_details.each do |user_detail| %>
      <tr>
        <td><%= user_detail.username %></td>
        <td><%= user_detail.firstname %></td>
        <td><%= user_detail.middlename %></td>
        <td><%= user_detail.lastname %></td>
        <td><%= user_detail.emailAddress %></td>
        <td><%= user_detail.password %></td>
        <td><%= user_detail.phoneno %></td>
        <td><%= user_detail.dateOfBirth %></td>
        <td><%= link_to 'Show', user_detail %></td>
        <td><%= link_to 'Edit', edit_user_detail_path(user_detail) %></td>
        <td><%= link_to 'Destroy', user_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Detail', new_user_detail_path %>
